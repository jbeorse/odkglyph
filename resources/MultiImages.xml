<h:html xmlns="http://www.w3.org/2002/xforms"
        xmlns:h="http://www.w3.org/1999/xhtml"
        xmlns:ev="http://www.w3.org/2001/xml-events"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:jr="http://openrosa.org/javarosa">
	<h:head>
		<h:title>Demo Form A</h:title>
		<model>
		   
			
		     <instance>
				
				<student>
					<questionA />
					<questionB />
				</student>
			</instance>
			<!--namespace: jr:iimage-->
			<!-- The form works assuming that the images are in the correct sdcard path-->
			<iimage>
				<imageSet setName="Q1">
					<image id="img_1">/sdcard/gallery_photo_1.jpg</image>			
					<image id="img_2">/sdcard/gallery_photo_2.jpg</image>	
					<image id="img_3">/sdcard/gallery_photo_3.jpg</image>		
				</imageSet>
                <imageSet setName="Q2">
					<image id="img_4">/sdcard/gallery_photo_4.jpg</image>		
					<image id="img_5">/sdcard/gallery_photo_5.jpg</image>			
					<image id="img_6">/sdcard/gallery_photo_6.jpg</image>					
                </imageSet>
			</iimage>
		</model>
	</h:head>

	<h:body>
		<input ref="questionA">
			<label img="jr:iimage('Q1')">What do you think about these photos?</label>
		</input>
	
		<input ref="questionB">
			<label img="jr:iimage('Q2')">How about these photos?</label>
		</input>

	</h:body>
</h:html>
